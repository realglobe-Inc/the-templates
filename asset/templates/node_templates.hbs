/**
 * Template contents
 * @namespace Templates
{{#each files}}
 * @property {string} {{{name}}} - Template from {{filename}}
{{/each}}
 */
'use strict'

const { compile } = require('handlebars')
const Templates = {}

{{#each files}}
// From {{filename}}
Templates['{{{name}}}'] = compile(`{{{content}}}`)

{{/each}}
module.exports = Templates
